@using SportManager.Models
@using Microsoft.AspNetCore.Http;
@{
    String UserType = "",UserName="";
    Student student = null;
    Staff staff = null;

    try
    {
        UserType = Context.Session.GetString("USERTYPE");
        if(UserType.ToUpper().Equals("STAFF"))
            staff = SessionHelper.GetObjectFromJson<Staff>(Context.Session, "MY_l_USER");
        else
            student = SessionHelper.GetObjectFromJson<Student>(Context.Session, "MY_l_USER");

        UserName = (staff != null) ? staff.Firstname + " " + staff.Lastname : student.Firstname + " " + student.Lastname;
    }
    catch (Exception ex) { }

}
