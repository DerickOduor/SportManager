    <script>
        $(document).ready(function () {
            $('#profileForm').on('sumbit', function (e) {
                e.preventDefault();
                var profileRegex = /^[a-zA-Z0-9]+(([._ -][a-zA-Z0-9 ])?[a-zA-Z0-9]*)*$/;
                if (!profileRegex.test($('#profileName').val())) {
                    $('.profileNamemessages').html('Invalid profile name!');
                    return;
                }

                $(this).submit();
            });
        });
    </script>
    <script>
        $(function () {
            $('#SportCategoryId').on('change', function (e) {
                var id = $(this).val();
                $('#StoreItemId').html('<option disabled selected>--select--</option>');
                $.ajax({
                    url: '/StoreRequest/FetchStoreItems/'+id,
                    method: 'get',
                    data: '',
                    contentType: 'application/json',
                    success: function (msg)
                    {
                        console.log(JSON.stringify(msg));
                        if (JSON.stringify(msg) != '') {
                            //console.log(JSON.stringify(msg));
                            console.log("Not empty");
                            data = $.parseJSON(msg);
                            /*$('#notification_list').append(
                                '<div><h3>Policy Applications</h3></div>'
                            )*/
                            $.each(data, function (i, item) {
                                //alert(item);
                                //notification_count += 1;
                                $('#StoreItemId').append(
                                    '<option value="'+item.Id+'">'+item.Name+'</option>'
                                )
                            });
                        } else {
                            console.log("Empty");
                        }
                    },
                    error: function (err) {
                        console.log(JSON.stringify(er));
                    }
                });
            });
            $('#StoreItemId').on('change', function (e) {
                var id = $(this).val();
                $.ajax({
                    url: '/StoreRequest/FetchStoreItem/'+id,
                    method: 'get',
                    data: '',
                    contentType: 'application/json',
                    success: function (msg)
                    {
                        if (JSON.stringify(msg) != '') {
                            console.log("Not empty");
                            data = $.parseJSON(msg);
                            $.each(data, function (i, item) {
                                //notification_count += 1;
                                $('#ItemQty').html(
                                    item.Count
                                )
                            });
                        } else {
                            console.log("Empty");
                        }
                    },
                    error: function (err) {
                        console.log(JSON.stringify(er));
                    }
                });
            });

            $('#motorPolicyTypeCodeDiv').hide();
            $('input.parent_menu').click(function () {
                var id = $(this).attr('id');
                $('.sub_menu.' + id).each(function () {
                    $(this).attr('checked', !$(this).attr('checked'));
                });
            });

            $('select.Right#ProfileId').on('change', function (e) {
                var id = $(this).val();
                if (id == '--select--')
                    return;
                window.location = '/AccessRights/Create/'+id;
            });

            $('#packageMainClassCode').on('change', function (e) {
                var code = $(this).val();
                if (code == '005') {
                    $('#motorPolicyTypeCodeDiv').show();
                } else {
                    $('#motorPolicyTypeCodeDiv').hide();
                }
            });
            //fetchPolicies();
            //setInterval(fetchPolicies, 120000);
            function fetchPolicies() {
                var notification_count = 0;
                $.ajax({
                    url: '/PolicyApplication/FetchPolicyApplications',
                    data: '',
                    contentType: 'application/json',
                    method: 'get',
                    success: function (msg) {
                        //console.log(JSON.stringify(msg));
                        if (JSON.stringify(msg) != '') {
                            console.log("Not empty");
                            data = $.parseJSON(msg);
                            /*$('#notification_list').append(
                                '<div><h3>Policy Applications</h3></div>'
                            )*/
                            $.each(data, function (i, item) {
                                //alert(item);
                                notification_count += 1;
                                $('#notification_list').append(
                                    '<li>' +
                                    '<div class="media">' +
                                    //'<img class="img-radius" src="~/assets/jpg/avatar-4.jpg" alt="Generic placeholder image">' +
                                    '<div class="media-body">' +
                                    '<h5 class="notification-user">Policy Application</h5>' +
                                    '<p class="notification-msg">' +
                                    '<div><b>Policy App No.:</b>&nbsp;' + item.appNo + '</div>' +
                                    '<div><b>Product:</b>&nbsp;' + item.productCode + '</div>' +
                                    '</p> ' +
                                    '<span class="notification-time"><b>Time:</b>&nbsp;' + item.createDate + '</span>' +
                                    '<span class="notification-time"><a href="/PolicyApplication/Details/' + item.id + '" id="view_policy_applicatio">View</a></span>' +
                                    '</div>' +
                                    '</div>' +
                                    '</li>'
                                )
                            });
                            $('#notification_count').html(notification_count);
                        } else {
                            console.log("Empty");
                        }
                    },
                    error: function (er) {
                        console.log(JSON.stringify(er));
                    }

                });
            }
        });
    </script>

